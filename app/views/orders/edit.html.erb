<%= javascript_include_tag 'users' %>
<h1>Current Order</h1>

<%= form_for @order do |f| %>
  <%= f.fields_for :products do |builder| %>
    <%= render "product_fields", :f => builder %>
  <% end %>
  <p><%= link_to_add_fields "Add Product", f, :products %></p>
  <p><%= f.submit "Submit" %></p>
<% end %>
